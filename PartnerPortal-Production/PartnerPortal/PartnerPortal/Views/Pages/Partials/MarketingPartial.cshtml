    app.controller("StarSupportResellerController", function ($controller, $scope, $http, $sce,
        notificationFactory, $location,
        validationFactory, filterFilter, helperFactory, dbUtilFactory, $filter) {


        $scope.Library = [];
        $scope.LibraryFiles = [];
        $scope.MarketingMaterials = [];
        $scope.Resources = [];
        $scope.showLoading = true;
        $scope.showNoDataBattleCards = false;
        $scope.showNoDataProducts = false;
        $scope.showNoDataResources = false;
        $scope.showData = false;

        $scope.showDownloadButton = true;

        $scope.ShowProducts = true;
        $scope.ShowResources = false;
        $scope.ShowBattleCards = false;

        $scope.ProductPagingMaxSiize = 1;
        $scope.ResourcePagingMaxSiize = 1;
        $scope.BattleCardsPagingMaxSiize = 1;

        $scope.articleHover = function () { }

        $scope.formatDate = function (date, format) {
            if (date == undefined) return '';
            return $.format.date(new Date(date), format);
        };

        $scope.SwitchTab = function (val) {
            switch (val) {
                case 1:
                    {
                        $scope.ShowProducts = true;
                        $scope.ShowResources = false;
                        $scope.ShowBattleCards = false;
                        $scope.searchText = '';
                        break;
                    }
                case 2:
                    {
                        $scope.ShowProducts = false;
                        $scope.ShowResources = true;
                        $scope.ShowBattleCards = false;
                        $scope.searchText = '';
                        break;
                    }
                case 3:
                    {
                        $scope.ShowProducts = false;
                        $scope.ShowResources = false;
                        $scope.ShowBattleCards = true;
                        $scope.searchText = '';
                        break;
                    }
                default:
                    {
                        $scope.ShowProducts = true;
                        $scope.ShowResources = false;
                        $scope.ShowBattleCards = false;
                        break;
                    }
            }
        }

        $scope.getLibrary = function () {
            var url = root + 'api/ppcmsapi/GetLibraryFilesForMarketing';
            dbUtilFactory.get(url, "", function (d) {
                $scope.LibraryFiles = d;
                $scope.showLoading = false;

                $scope.$watch('searchText', function (term) {
                    $scope.LibraryFiles = $filter('filter')(d, function (item) {
                        var regex = new RegExp($scope.searchText, 'gi');
                        return item.Title.match(regex);
                    });
                });

                if (d.length <= 0) {
                    $scope.showNoDataBattleCards = true;
                }
                else {
                    $scope.showData = true;
                    $("#dataContainerBattleCards").show();
                    $scope.BattleCardsPagingMaxSiize = d.length / 3;
                }

            }, '', 'Error getting data ');

            var url = root + 'api/ppcmsapi/GetImportedMarketingMaterial';
            dbUtilFactory.get(url, "", function (d) {
                $scope.MarketingMaterials = d;
                $scope.showLoading = false;

                $scope.$watch('searchText', function (term) {
                    $scope.MarketingMaterials = $filter('filter')(d, function (item) {
                        var regex = new RegExp($scope.searchText, 'gi');
                        return item.Name.match(regex) || item.ProductTagline.match(regex);
                    });
                });

                if (d.length <= 0) {
                    $scope.showNoDataProducts = true;
                }
                //if ($scope.MarketingMaterials.DataSheetDownLoadLink == null) {
                //    $scope.showDownloadButton = false;
                //}

                else {
                    $("#dataContainerMarketing").show();
                    $scope.ProductPagingMaxSiize = d.length / 3;
                }

            }, '', 'Error getting data ');

            var url = root + 'api/ppcmsapi/GetImportedResources';
            dbUtilFactory.get(url, "", function (d) {
                $scope.Resources = d;
                $scope.showLoading = false;

                $scope.$watch('searchText', function (term) {
                    $scope.Resources = $filter('filter')(d, function (item) {
                        var regex = new RegExp($scope.searchText, 'gi');
                        return item.Title.match(regex) || item.ResourceType.match(regex);
                    });
                });

                if (d.length <= 0) {
                    $scope.showNoDataResources = true;
                } else {
                    $scope.ResourcePagingMaxSiize = d.length / 3;
                    $("#dataContainerMarketing").show();
                }

            }, '', 'Error getting data ');
        };
        $scope.getLibrary();

        $scope.ShowLibraryFiles = function (index, show) {
            if (show) return true;
            if (!show) {
                if (index < 4) return true;

            }
            return false;
        }
