<script>
     

        app.controller("StarDistResellersController", function ($controller, $scope, $http, $sce, 
            notificationFactory, $location,
            validationFactory, filterFilter, helperFactory, dbUtilFactory) {
            var d = new Date();
            $scope.TodayDate = new Date();//d.getMonth() + "/" + d.getDate() + "/" + d.getFullYear();
            $scope.ResellersToExport = [];
            $scope.ResellersToExportSearched = [];
            $scope.disclaimerList = [];
            $scope.showLoading = true;
            $scope.ExportFileName = 'ResellerList.csv';
            $scope.Account = {};
            $scope.disclaimerLine = "";

            $scope.getTime = function () {
                var d = new Date( ); 
                d.setHours( d.getHours()); // offset from local time
                var h = (d.getHours() % 12) || 12; // show midnight & noon as 12
                return (
                    ( h < 10 ? '0' : '') + h +
                    ( d.getMinutes() < 10 ? ':0' : ':') + d.getMinutes() +
                            // optional seconds display
                    // ( d.getSeconds() < 10 ? ':0' : ':') + d.getSeconds() + 
                    ( d.getHours() < 12 ? ' AM' : ' PM' )
                );
	
            }


            $scope.ColumnList = { CustomerName: 'Account #', ResellerAccountName: 'Reseller Name', ResellerPartnerType: 'Partner Type', SuggestedMarkup: "Suggested Markup", DistributorDiscount: "Distributor DRD", ResellerDiscount: 'Reseller’s  Discount', ResellerPartnerNumber: 'Partner Number', DateCreated: 'Date Added' };
            $scope.ColumnListSearched = { ResellerAccountName: 'Reseller Name', ResellerPartnerType: 'Partner Type', SuggestedMarkup: "Suggested Markup", DistributorDiscount: "Distributor DRD", ResellerDiscount: 'Reseller’s  Discount', ResellerPartnerNumber: 'Partner Number', DateCreated: 'Date Added' };

         //   $scope.disclaimerList = { line: $scope.disclaimerLine1, line1: $scope.disclaimerLine2, line2: $scope.disclaimerLine3 };
           
            $scope.getAccount = function () {
                dbUtilFactory.get(root + 'api/ppcmsapi/GetAccountFromSessionData/', "", function (d) {
                    $("#StarDistResellersControllerContent").show();
                    $scope.Account = d;
                    $scope.disclaimerLine = "(c) " + (new Date()).getFullYear() + " Star Micronics America. All rights reserved.";
                    $scope.disclaimerList.push({ description: $scope.disclaimerLine });
                    $scope.disclaimerLine = "Confidential Information - Do Not Distribute.";
                    $scope.disclaimerList.push({ description: $scope.disclaimerLine });
                    $scope.disclaimerLine = "Generated By: " + $scope.Account.AccountName + " " + ((new Date()).getMonth() + 1) + "/" + (new Date()).getDate() + "/" + (new Date()).getFullYear() + " " + $scope.getTime();
                    $scope.disclaimerList.push({ description: $scope.disclaimerLine });
                }, '', 'Error getting data ');
            };
            $scope.getAccount();

            $scope.searchPartnerForReseller = function () {
                var obj = {};
                obj.ResellerAccountName = $("#tdResellerAccountName").html();
                obj.ResellerPartnerType = $("#tdResellerPartnerType").html();
                obj.SuggestedMarkup = $("#tdResellerSuggestedMarkup").html();
                obj.DistributorDiscount = $("#tdResellerDistDrd").html();
                obj.ResellerDiscount = $("#tdResellerDiscount").html();
                obj.ResellerPartnerNumber = $("#tdResellerPartnerNumber").html();
                $scope.ResellersToExportSearched.push(obj);
            }

            $scope.getResellers = function() {
                dbUtilFactory.get(root + 'api/ppcmsapi/GetPurchaseByDistributorFromSessionData/', "", function (d) {
                    $scope.Resellers = d;
                    $scope.ResellersToExport = d;
                    $scope.showLoading = false;
                }, '', 'Error getting data ');
            };
            $scope.getResellers();
            $scope.exportExcel = function () {
                //2 execute the hidden button 
                document.getElementById('btnExport').click();
                //$scope.GetResellersToExport();
            }
            $scope.exportExcelSearched = function () {
                //2 execute the hidden button 
                document.getElementById('btnExportSearched').click();
                //$scope.GetResellersToExport();
            }

            $scope.$watch('filterName', function (newVal, oldVal, scope) {
                var query = $("#filter").val();
                if (query != undefined && query != "") {
                    var temp = [];
                    for (var i = 0; i < $scope.Resellers.length; i++) {
                        if ((($scope.Resellers[i].ResellerAccountName != null) && (angular.lowercase($scope.Resellers[i].ResellerAccountName).indexOf(angular.lowercase(query) || '') !== -1)) ||
                            (($scope.Resellers[i].CustomerName != null) && (angular.lowercase($scope.Resellers[i].CustomerName).indexOf(angular.lowercase(query) || '') !== -1))) {
                            temp.push($scope.Resellers[i]);
                        }
                    }
                    $scope.ResellersToExport = temp;

                } else {
                    $scope.ResellersToExport = $scope.Resellers;
                }
            });

            $scope.doFilter = function (row) {
                return ((row.ResellerAccountName != null) && (angular.lowercase(row.ResellerAccountName).indexOf(angular.lowercase($scope.filterName) || '') !== -1) ||
                ((row.CustomerName != null) && (angular.lowercase(row.CustomerName).indexOf(angular.lowercase($scope.filterName)) || '') !== -1));

            }

        });
    </script>
