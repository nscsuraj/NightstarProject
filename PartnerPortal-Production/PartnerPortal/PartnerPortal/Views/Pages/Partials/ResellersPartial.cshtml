<div id="StarDistResellersControllerContent" style="width: 100%; display: none;" ng-controller="StarDistResellersController">
    <div class="fusion-fullwidth fullwidth-box" style="text-align: left; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover;">

        <div class="fusion-row" id="displayResellers">
            <div style="width: 100%; display: inline-block; text-align: left; padding-top: 30px; color: #3D80EB;">
                <div style="width: 100%; height: 30px; color: #2551A4; margin-top: 40px; font-size: 22pt; font-weight: bold;" class="twelveMonthReseller">
                    Your 12 Month Resellers
                </div>
                <div style="width: 100%; height: 30px; color: #2551A4; font-size: 12pt; font-weight: normal;line-height: 1.5">
                    Partners who you currently have business with are listed below.
                </div>
                <div class="oppHeaderCol1" style="margin-top: 20px;">
                    <div style="width: 100%; color: #2551A4; font-size: 12pt;">Filter by Reseller Name & Account #</div>
                    <div style="width: 100%; float: left; display: table-cell; vertical-align: middle;">
                        <input class="clsTextBoxSearch" id="filter" placeholder="&#128269;" type="text" value="" ng-model="filterName">
                    </div>
                </div>
                <div class="oppHeaderCol2" style="margin-top: 20px; display: none;">
                    <div style="width: 100%; color: #2551A4; font-size: 12pt;">&nbsp;</div>
                    <div style="width: 100%; float: left; display: table-cell; vertical-align: middle;">
                        <table style="width: 100%;">
                            <tr style="color:#8C9D41">
                                <td style="font-size: 14pt !important; font-weight: bold; letter-spacing: 1px;">Distributor Discount</td>
                                <td style="width: 50px; height: 40px; background-color: #D3ECFA; text-align: center;font-size: 14pt !important; font-weight: bold; letter-spacing: 1px;">{{Account.DistributorDiscount}}</td>
                            </tr>
                        </table>
                    </div>
                </div>

            </div>
            <div style="width: 100%; display: inline-block; text-align: left; padding-top: 30px; color: #3D80EB;">
                <div style="width: 50%; padding-left: 15px; float: left;">
                    <div style="width: 100%; color: #2551A4; font-weight: bold; font-size: 12pt;line-height: 1.5">Effective date : {{TodayDate | date : "MM/dd/yyyy"}}</div>
                </div>
                <div style="width: 30%; padding-left: 15px;padding-right: 25px; float: right; text-align: right;">
                    <div class="btnExport" ng-click="exportExcel()" style="cursor: pointer;">Export</div>
                     <button ng-json-export-excel data="ResellersToExport" report-fields="ColumnList" optional-Texts ="disclaimerList" filename="'ResellerList'" style="display: none;" id="btnExport">Export Excel</button>
                </div>
            </div>
            <div id="displayOpportunityList" style="padding: 10px; text-align: center;">
                <table style="cursor: pointer; text-align: left; width: 100%; table-layout: fixed; word-wrap: break-word;">
                    <thead>
                    <tr style="text-align: left; border-bottom: 1px solid #000000;">
                        <th class="oppTableHeader tabColCustName" style="padding-left: 5px;">Account #</th>
                        <th class="oppTableHeader tabColReseller" style="padding-left: 5px;">Reseller Name</th>
                        <th class="oppTableHeader tabColPartnerType" style="padding-left: 5px;">Partner Type</th>
                        <th class="oppTableHeader">Suggested Markup</th>
                        <th ng-click="predicate = 'OpportunityName'; reverse=!reverse" class="oppTableHeader tabColDiscount">Distributor DRD</th>
                        <th ng-click="predicate = 'OpportunityName'; reverse=!reverse" class="oppTableHeader tabColPartDist">Reseller’s Discount</th>
                        <th class="oppTableHeader tabColPartnerNo" style="text-align: left;">Partner Number</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-show="showLoading">
                        <td colspan="6">
                            <div style="font-size: 16pt; font-weight: bold; color: #737373; margin-left: 15px; margin-bottom: 20px; border: 1px solid #ececec; text-align: center;">
                                <img src="~/Images/loadingP.gif"/>
                            </div>

                        </td>
                    </tr>
                    <tr dir-paginate-start="item in Resellers | filter : doFilter | orderBy : predicate:reverse|itemsPerPage:100" ng-hide="showLoading">
                        <td style="height: 10px; padding: 0; font-size: 4pt; background-color: #ECF8FF;" colspan="6">&nbsp;</td>
                    </tr>
                    <tr style="background-color: #D3ECFA; color: #001E4F; height: 30px;">
                        <td class="oppTableText" style="padding-left: 5px;">{{item.CustomerName}}</td>
                        <td class="oppTableText" style="padding-left: 5px;">{{item.ResellerAccountName}}</td>
                        <td class="oppTableText" style="padding-left: 5px;">{{item.ResellerPartnerType}}</td>
                        <td class="oppTableText" style="padding-left: 5px;">{{item.SuggestedMarkup}}</td>
                        <td class="oppTableText" style="padding-left: 5px;">{{item.DistributorDiscount}}</td>
                        @*<td class="oppTableText" style="padding: 5px;">{{item.ResellerDiscount}}</td>*@
                        <td class="oppTableText" style="padding: 5px;">-</td>
                        <td class="oppTableText" style="padding: 5px; text-align: left;">{{item.ResellerPartnerNumber}}</td>
                    </tr>
                    <tr dir-paginate-end>
                        <td style="height: 1px; padding: 0; font-size: 0pt; background-color: #ECF8FF;" colspan="6"></td>
                    </tr>

                    </tbody>
                </table>
                <dir-pagination-controls max-size="5" direction-links="true" boundary-links="true"></dir-pagination-controls>
            </div>


        </div>
        <div id="displaySearchPartner" class="fusion-row" style="display: none;">
            <div style="width: 100%; display: inline-block; text-align: left; padding-top: 30px; color: #3D80EB;">
                <div style="width: 50%; padding-left: 15px; float: left;">
                    <div style="width: 100%; color: #2551A4; font-weight: bold; font-size: 12pt;line-height: 1.5"></div>
                </div>
                <div style="width: 30%; padding-left: 15px;padding-right: 25px; float: right; text-align: right;">
                    <div class="btnExport" ng-click="exportExcelSearched()" style="cursor: pointer;">Export</div>
                     <button ng-json-export-excel data="ResellersToExportSearched" report-fields="ColumnListSearched" optional-Texts ="disclaimerList" filename="'ResellerList'" style="display: none;" id="btnExportSearched">Export Excel</button>
                </div>
            </div>
            <div style="padding: 10px; text-align: center;">
                <table style="cursor: pointer; text-align: left; width: 100%; table-layout: fixed; word-wrap: break-word;">
                    <thead>
                    <tr style="text-align: left; border-bottom: 1px solid #000000;">
                        <th class="oppTableHeader tabColReseller" style="padding-left: 5px;">Reseller Name</th>
                        <th class="oppTableHeader tabColReseller" style="padding-left: 5px;">Partner Type</th>
                        <th class="oppTableHeader">Suggested Markup</th>
                        <th ng-click="predicate = 'OpportunityName'; reverse=!reverse" class="oppTableHeader tabColDiscount">Distributor DRD</th>
                        <th ng-click="predicate = 'OpportunityName'; reverse=!reverse" class="oppTableHeader tabColPartDist">Reseller’s Discount</th>
                        <th class="oppTableHeader tabColPartnerNo" style="text-align: left;">Partner Number</th>
                    </tr>
                    </thead>
                    <tbody>
                        <tr style="background-color: #D3ECFA; color: #001E4F; height: 30px;">
                            <td class="oppTableText" style="padding-left: 5px;" id="tdResellerAccountName"></td>
                            <td class="oppTableText" style="padding-left: 5px;" id="tdResellerPartnerType"></td>
                            <td class="oppTableText" style="padding-left: 5px;" id="tdResellerSuggestedMarkup"></td>
                            <td class="oppTableText" style="padding-left: 5px;" id="tdResellerDistDrd"></td>
                            @*<td class="oppTableText" style="padding: 5px;" id="tdResellerDiscount"></td>*@
                            <td class="oppTableText" style="padding: 5px;" id="tdResellerDiscountDemo">-</td>
                            <td class="oppTableText" style="padding: 5px; text-align: left;" id="tdResellerPartnerNumber"></td>
                        </tr>

