<script>
     

        app.controller("StarDistContentController", function ($controller, $scope, $http, $sce, 
            notificationFactory, $location,
            validationFactory, filterFilter, helperFactory, dbUtilFactory) {


            $scope.OpportunitiesCount = 0;
            $scope.ResellerCount = 0;

            $scope.showOpportunitySection = false;

            $scope.Account = {};
            $scope.getAccount = function () {
                dbUtilFactory.get(root + 'api/ppcmsapi/GetAccountFromSessionData/', "", function (d) {
                    $scope.Account = d;
                }, '', 'Error getting data ');
            };
            $scope.getAccount();
            $scope.formatDate = function(date, format) {
                if (date == undefined)return '';
                return $.format.date(new Date(date), format);
            };

            $scope.getOpportunities = function() {
                dbUtilFactory.get(root + 'api/ppcmsapi/GetOpportunitiesCountSessionData/', "", function (d) {
                    $scope.OpportunitiesCount = d;
                    $scope.showOpportunitySection = true;
                    $("#StarDistContentController").show();

                }, '', 'Error getting data ');
            };
            $scope.getOpportunities();

            $scope.getResellers = function () {
                dbUtilFactory.get(root + 'api/ppcmsapi/GetPurchaseByDistributorCountFromSessionData/', "", function (d) {
                    $scope.ResellerCount = d;
                }, '', 'Error getting data ');
            };
            $scope.getResellers();

        });
