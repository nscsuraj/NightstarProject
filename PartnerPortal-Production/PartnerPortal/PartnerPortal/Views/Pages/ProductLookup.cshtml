@model PartnerPortal.Models.PageInfoVM
@{
    ViewBag.Title = Model.TitleTag;
    Layout = "~/Views/Shared/_LayoutReseller.cshtml";
}

@section MetaTags
{
    <link rel="canonical" href="https://www.starmicronics.com/partnerportal/pages/pricelookup/" />
     @foreach (var m in Model.Gateway.MetaTags)
     {
         if (m.TagType == 0)
         {
             <meta property="og:@m.TagKey" content="@m.TagValue"/>
         }
         else
         {
            <meta name="@m.TagKey" content="@m.TagValue"/>
         }
     
     }
}

@section scripts
{
    <style>
        @Html.Action("GetFileContent", "Utility", new {path = Url.Content("~/Styles/bootstrap.css")})
    </style>
}
@section MainContent {
    
    @Html.Partial("Partials/ProductLookupPartial")

    @foreach (var page in Model.PageSections)
    {
        @Html.Partial("Partials/PagePartial", page.Id)
    }

        @Html.HiddenFor(m=> m.MenuName)
        @Html.HiddenFor(m=> m.PageHeader)
}